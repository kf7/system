{
  "name": "kf7/system",
  "description": "PHP-7-compatible replacement for Kohana/Koseven framework",
  "keywords": ["kf7", "kohana7", "koseven", "ko7", "kohana", "framework", "php"],
  "version": "0.0.1",
  "type": "library",
  "license": "BSD-3-Clause",
  "homepage": "https://kf7.github.io",
  "authors": [{"name": "Kohana team", "homepage": "https://kf7.github.io/team", "role": "developer"}],
  "support": {
    "source": "https://github.com/kf7/system",
    "issues": "https://github.com/kf7/system/issues",
    "docs": "https://kf7.github.io/modules/system",
    "forum": "https://kf7.discourse.group",
    "chat": "https://telegram.me/kf7_chat"
  },
  "require": {
    "php": "^7.1",
    "ext-dom": "*",
    "symfony/polyfill-ctype": "^1.13",
    "symfony/polyfill-php72": "^1.13",
    "symfony/polyfill-php73": "^1.13",
    "symfony/polyfill-php74": "^1.13",
    "symfony/polyfill-mbstring": "^1.13",
    "symfony/polyfill-iconv": "^1.13",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
      "psr-4": {
        "KF7\\System\\": "src/System/"
      }
  },
  "autoload-dev": {
      "psr-4": {
        "KF7\\System\\Test\\": "tests/"
      }
  }
}

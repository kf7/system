{
    "name": "kf7/system",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "PHP-7-compatible replacement for Kohana/Koseven framework",
    "keywords": [
        "system",
        "kf7",
        "kohana",
        "kohana7",
        "framework",
        "php"
    ],
    "homepage": "https://kf7.github.io",
    "support": {
        "source": "https://github.com/kf7/system",
        "issues": "https://github.com/kf7/system/issues",
        "docs": "https://kf7.github.io/modules/system",
        "forum": "https://kf7.discourse.group",
        "chat": "https://telegram.me/kf7_chat"
    },
    "authors": [
        {
            "name": "Kohana7 team",
            "homepage": "https://kf7.github.io/team",
            "role": "developer"
        },
        {
            "name": "Koseven Team",
            "homepage": "https://koseven.dev/",
            "role": "developer"
        },
        {
            "name": "Kohana Team",
            "email": "team@kohanaframework.org",
            "homepage": "http://kohanaframework.org/team",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "ext-json": "*",
        "symfony/polyfill-ctype": "^1.17",
        "symfony/polyfill-iconv": "^1.17",
        "symfony/polyfill-intl": "^1.17",
        "symfony/polyfill-php73": "^1.17",
        "symfony/polyfill-php74": "^1.17",
        "psr/log": "^1.0",
        "psr/simple-cache": "^1.0",
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "php-coveralls/php-coveralls": "^2.1",
        "phpdocumentor/phpdocumentor": "^2.9",
        "phing/phing": "^2.16",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "suggest": {
        "ext-ctype": "Validate variables",
        "ext-iconv": "Convert strings encoding",
        "ext-intl": "Internationalize and normalize strings"
    },
    "autoload": {
        "psr-4": {
            "Kohana\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kohana\\": "tests/"
        }
    }
}

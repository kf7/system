    "docs": "https://kf7.github.io/modules/system",
    "forum": "https://kf7.discourse.group",
    "chat": "https://telegram.me/kf7_chat"
  },
  "require": {
    "php": "^7.2",
    "ext-mbstring": "*",
    "ext-libxml": "*",
    "symfony/polyfill-ctype": "^1.13",
    "symfony/polyfill-php73": "^1.13",
    "symfony/polyfill-php74": "^1.13",
    "symfony/polyfill-iconv": "^1.13",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
      "psr-4": {
